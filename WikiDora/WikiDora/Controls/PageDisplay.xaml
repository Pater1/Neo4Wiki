<UserControl x:Class="WikiDora.Controls.PageDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WikiDora.Controls"
             xmlns:controls="clr-namespace:WikiDora.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="#FFE2E2E2">
        <Button x:Name="btn_LikePage" Content="Like" Margin="90,258,135,0" VerticalAlignment="Top" Click="button_Click"></Button>
        <Button x:Name="btn_DislikePage" Content="Dislike" HorizontalAlignment="Left" Margin="10,258,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btn_NextPage" Content="Next Page" HorizontalAlignment="Left" Margin="215,258,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="textBlock_PageTitle" Margin="60,10,60,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="16"/>
        <Viewbox HorizontalAlignment="Left" Height="217" Margin="18,36,0,0" VerticalAlignment="Top" Width="272"/>

    </Grid>
</UserControl>
